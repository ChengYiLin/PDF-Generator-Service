{
    "compilerOptions": {
        "jsx": "react-jsx",
        "target": "esnext",
        "strict": true,
        "preserveConstEnums": true,
        "noEmit": true,
        "sourceMap": false,
        "module": "commonjs",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "exclude": ["node_modules", "**/*.test.ts"]
}
